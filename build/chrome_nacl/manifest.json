{
  "name": "Unreal Speccy Portable",
  "version": "0.0.49",
  "manifest_version": 2,
  "description": "Portable ZX Spectrum emulator supports Z80 128K (Pentagon) AY/YM, Beeper, BetaDisk, Tape, Kempston Joystick/Mouse, Snapshots.",
  "icons": {
    "16" : "unreal_speccy_portable_16.png",
    "48" : "unreal_speccy_portable_48.png",
    "128": "unreal_speccy_portable_128.png"
  },
  "app": {
    "launch": {
      "local_path": "unreal_speccy_portable.html"
    }
  },
  "permissions": [
	"http://vtrdos.ru/"
  ],
  "requirements": {
    "3D": {
    	"features": ["webgl"]
  	}
  },
  "minimum_chrome_version": "26"
}
